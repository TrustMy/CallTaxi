<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.trust.shengyu.calltaxi.activitys.MainMapActivity">





        <EditText
            android:id="@+id/map_order_start_name"
            android:hint="起点"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <EditText
            android:hint="终点"
            android:id="@+id/map_order_end_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />



        <Button
            android:text="order starts"
            android:id="@+id/map_order_starts"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


        <Button
            android:text="order ends"
            android:id="@+id/map_order_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    <include
        android:id="@+id/mainmap_map"
        layout="@layout/map_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>



</LinearLayout>
